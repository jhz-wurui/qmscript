[SetupUI]
Type=1
QUI=Form1
[Relative]
SetupOCXFile=
[Comment]

[UIPackage]



[Script]
Global 任务ID, 控制开关
//---------------------开始----------------------//
Event Form1.Button1.Click
    控制开关 = True
    任务ID = BeginThread(任务线程)
    TracePrint "任务ID: " & 任务ID
End Event
//---------------------任务线程-------------------//
Sub 任务线程()
    秒 = 0 : 分 = 0 : 时 = 0
    时_字 = "00" : 分_字 = "00" : 秒_字 = "00"
    //通过循环判断进行处理
    Do While 控制开关
        Form1.InputBox1.Text = 时_字 & ":" & 分_字 & ":" & 秒_字
        Delay 1000
        //先对秒进行计算
        秒 = 秒 + 1
        If 秒 = 60 Then 
            秒 = 0
            分 = 分 + 1
            //这里是对分 的计算
            If 分 = 60 Then 
                分 = 0
                时 = 时 + 1
            End If
        End If
        //对于数字是一位时 进行补0处理
        秒_字 = CStr(秒)
        If Len(秒_字) = 1 Then 
            秒_字 = "0" & 秒_字
        End If
        分_字 = CStr(分)
        If Len(分_字) = 1 Then 
            分_字 = "0" & 分_字
        End If
        时_字 = CStr(时)
        If Len(时_字) = 1 Then 
            时_字 = "0" & 时_字
        End If
    Loop
End Sub
//----------------------停止--------------------------//
Event Form1.Button2.Click
    控制开关 = False
    Form1.InputBox1.Text = "00:00:00"
    StopThread 任务ID
End Event
//----------------------暂停/继续---------------------//
Event Form1.Button3.Click
    If Form1.Button3.Caption = "暂停" Then 
        //暂停线程
        PauseThread 任务ID
        Form1.Button3.Caption = "继续"
    Else 
        ContinueThread 任务ID
        Form1.Button3.Caption = "暂停"
    End If
End Event
