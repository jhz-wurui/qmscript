[General]
SyntaxVersion=2
BeginHotkey=121
BeginHotkeyMod=0
PauseHotkey=19
PauseHotkeyMod=0
StopHotkey=123
StopHotkeyMod=0
RunOnce=1
EnableWindow=
MacroID=e0793833-9596-4ec2-b302-5225128fa2e7
Description=watchstore文件合成
Enable=0
AutoRun=0
[Repeat]
Type=0
Number=1
[SetupUI]
Type=1
QUI=Form1
[Relative]
SetupOCXFile=
[Comment]

[UIPackage]



[Script]
关键词path = Form1.BrowseBox3.Path
链接path = Form1.BrowseBox4.Path


关键词行 = 1


I = 0


Text = Plugin.File.ReadFileEx(关键词path)

txtArray = Split(Text, "|")

While Len(txtArray(I)) >= 1

    I = I + 1
    
Wend


Do
    关键词 = Lib.文件.读取指定行文本内容(关键词path, 关键词行)
    If 关键词 = "" Then 
        MessageBox "完成"
        EndScript 
    End If
    链接 = Lib.文件.读取指定行文本内容(链接path, 关键词行)
    If 链接 = "" Then 
        MessageBox "已无可操作链接!"
        EndScript 
    End If
    内容 = "<div><a href=""" & 链接 & """>" & 关键词 & "</a></div>"
    
    Call Plugin.File.WriteFileEx("c:\生成结果.txt", 内容)
    
    Form1.ProgressBar1.Value = cint((关键词行/I)*100)
	
    关键词行 = 关键词行 + 1
Loop

Event Form1.LoadOver
    Form1.ProgressBar1.Value = 0  
End Event

